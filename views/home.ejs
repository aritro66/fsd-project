                        <a href="/doctor"><img src="images/depreeess.png" /> </a>
                    </div>
                    <div class="text">
                        Dipression or anxiety
                    </div>

                </div>



        </section>

        <!-- Online consultation section ends -->

        <!-- deal section starts  -->

        <section class="deal">

            <div class="content">
                <h3>deal of the day</h3>
                <h1>Get a Appointment With World Best Doctor</h1>
                <p>You are in safe hands, choose the experts and get consulation online</p>
                <a href="/doctor" class="btn">Get Now</a>
            </div>

            <div class="image">
                
                <img src="images/doctor_img.png" alt="">
