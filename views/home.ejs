<body>

    <!-- header section starts  -->

    <%- include('./partials/navbar.ejs'); %>

        <!-- header section ends -->

        <!-- bottom navbar  -->

        <nav class="bottom-navbar">
            <a href="#home" class="fas fa-home"></a>
            <a href="#featured" class="fas fa-list"></a>
            <a href="#reviews" class="fas fa-comments"></a>
        </nav>

        <!-- home section starts  -->

        <section class="home" id="home">

            <div class="row">

                <div class="content">
                    <h3>Buy Medicines from here</h3>
                    <p>Get best quality medicines from us at reasonable prices.</p>
                    <a href="/products" class="btn">Buy now</a>
                </div>

                <div class="swiper image-slider">
                    <div class="swiper-wrapper">
                        <a href="#" class="swiper-slide"><img src="images/d1.png" alt=""></a>
                        <a href="#" class="swiper-slide"><img src="images/d2.png" alt=""></a>
                        <a href="#" class="swiper-slide"><img src="images/d3.png" alt=""></a>
                        <a href="#" class="swiper-slide"><img src="images/d4.png" alt=""></a>
                        <a href="#" class="swiper-slide"><img src="images/d5.png" alt=""></a>
                        <a href="#" class="swiper-slide"><img src="images/d6.png" alt=""></a>
                    </div>
                    <img src="images/stand.png" class="stand" alt="">
                </div>

            </div>

        </section>

        <!-- home section ense  -->

        <!-- icons section starts  -->

        <section class="icons-container">

            <div class="icons">

                <i class="fas fa-shipping-fast"></i>
                <div class="content">
                    <a href="#">
                        <h3>free shipping</h3>
                    </a>
                    <p>order over 100rs</p>
                </div>
            </div>

            <div class="icons">
                <i class="fas fa-lock"></i>
                <div class="content">
                    <a href="#">
                        <h3>secure payment</h3>
                    </a>
                    <p>100% secure payment</p>
                </div>
            </div>

            <div class="icons">
                <i class="fas fa-redo-alt"></i>
                <div class="content">
                    <a href="#">
                        <h3>easy returns</h3>
                    </a>
                    <p>10 days returns</p>
                </div>
            </div>

            <div class="icons">
                <i class="fas fa-headset"></i>
                <div class="content">
                    <a href="#">
                        <h3>24/7 support</h3>
                    </a>
                    <p>call us anytime</p>
                </div>
            </div>

        </section>

        <!-- icons section ends -->

        <!-- featured section starts  -->

        <section class="featured" id="featured">

            <h2 class="heading"> <span>EXPLORE - PRODUCTS </span> </h2>
            <div id="Sviewbutton">
                <button onclick="window.location.href='product';" class="viewbutton">View All</button>
            </div>
            <div class="swiper featured-slider">

                <div class="swiper-wrapper">
                    <% productdata.forEach(data=>{ %>
                        <div class="swiper-slide box">

                            <div class="image">
                                <img src="/<%= data.img %>" alt="">
                            </div>
                            <div class="content">
                                <h3><%= data.name %></h3>
                                <div class="price">
                                    <%= data.price %>
                                </div>
                                <a href="product/<%= data._id %>" class="btn">add to cart</a>
                            </div>
                        </div>
                        <% }) %>

                </div>

                <div class="swiper-button-next"></div>
                <div class="swiper-button-prev"></div>

            </div>

        </section>

        <!-- featured section ends -->

        <!-- hospital search section starts -->        

        <!-- hospital search section ends -->

        <!-- Online consultation section starts  -->
        <section id="consultation">
            <h1 class="heading1"><span> Consult top doctors online for any health concern </span></h1>
            <p> Private online consultations with verified doctors in all specialists</P>
            <div class="consult-container">
                <!-- fist box start here  -->
                <div class="box">

                    <div class="image">
                        <a href="/doctor"> <img src="/images/cough image.png" /> </a>
                    </div>
                    <div class="text">
